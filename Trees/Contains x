bool isPresent(TreeNode<int>* root, int x) 
{
    // Write your code here
    if(root == NULL) return false;
    else if(root->data == x) return true;
    else
    {
        bool a = false;
        for(int i = 0; i < root->children.size(); i++)
        {
            a = a or isPresent(root->children[i],x);
        }
        return a;
    }
}
